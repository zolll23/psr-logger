{
  "name": "vpa/psr-logger",
  "description": "Implementations of PSR-3 Logger Interface for Developer Console and CLI console",
  "keywords": [
    "php 8.x",
    "PSR-3",
    "Logger",
    "CLI console logger",
    "JS console logger",
    "Injectable"
  ],
  "license": "AGPL-3.0-or-later",
  "homepage": "https://github.com/zolll23/psr-logger",
  "authors": [
    {
      "name": "Andrey Pakhomov",
      "email": "andrey.pahomov@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0",
    "psr/log": "^3.0",
    "vpa/di": "^0.1.0",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^4.8"
  },
  "autoload": {
    "psr-4": {
      "VPA\\": "src/VPA"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VPA\\": "tests"
    }
  },
  "scripts": {
    "cbf": "phpcbf",
    "static": "psalm",
    "cs-check": "phpcs",
    "test": "phpunit --coverage-clover clover.xml",
    "check": [
      "@cs-check",
      "@static",
      "@test"
    ]
  }
}
